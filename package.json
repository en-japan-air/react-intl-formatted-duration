{
  "name": "react-intl-formatted-duration",
  "version": "3.1.0",
  "description": "React intl component to express time duration",
  "keywords": [
    "duration",
    "react",
    "intl",
    "format"
  ],
  "homepage": "https://github.com/en-japan-air/react-intl-formatted-duration",
  "license": "MIT",
  "main": "dist/bundle.js",
  "module": "dist/module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/en-japan-air/react-intl-formatted-duration"
  },
  "files": [
    "dist",
    "index.js",
    "messages.js"
  ],
  "scripts": {
    "compile": "rollup -c rollup.config.js",
    "codecov": "codecov",
    "lint": "eslint index.js __tests__",
    "storybook": "start-storybook",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "prepare": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "codecov": "3.6.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "4.3.1",
    "react-test-renderer": "16.13.1",
    "rollup": "2.4.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "7.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "storybook-addon-intl": "2.4.1"
  },
  "peerDependencies": {
    "react": ">= 16.0",
    "react-intl": ">= 4.0",
    "intl-messageformat": ">= 2.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/internals/testing/test-setup.js"
    ],
    "testRegex": "__tests__/.*\\.test\\.js$",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "intl-unofficial-duration-unit-format": "1.1.0"
  }
}
