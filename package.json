{
  "name": "react-intl-formatted-duration",
  "version": "2.0.2",
  "description": "React intl component to express time duration",
  "keywords": [
    "duration",
    "react",
    "intl",
    "format"
  ],
  "homepage": "https://github.com/en-japan-air/react-intl-formatted-duration",
  "license": "MIT",
  "main": "dist/bundle.js",
  "module": "dist/module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/en-japan-air/react-intl-formatted-duration"
  },
  "files": [
    "dist",
    "index.js",
    "messages.js"
  ],
  "scripts": {
    "flow": "flow",
    "compile": "rollup -c rollup.config.js",
    "codecov": "codecov",
    "lint": "eslint index.js __tests__",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "prepare": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-react-intl": "^2.0.3",
    "eslint": "^5.8.0",
    "eslint-plugin-flowtype": "^3.1.4",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.85.0",
    "intl-unofficial-duration-unit-format": "1.0.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-intl": "^2.7.2",
    "react-test-renderer": "^16.6.0",
    "rollup": "0.66.6",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-filesize": "5.0.1",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-flow-entry": "0.2.0",
    "rollup-plugin-node-resolve": "3.4.0"
  },
  "peerDependencies": {
    "prop-types": ">= 15.0",
    "react": ">= 15.0",
    "react-intl": ">= 2.0",
    "intl-messageformat": ">= 2.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-setup.js",
    "testRegex": "__tests__/.*\\.test\\.js$",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  }
}
