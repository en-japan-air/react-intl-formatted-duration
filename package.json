{
  "name": "react-intl-formatted-duration",
  "version": "3.1.0",
  "description": "React intl component to express time duration",
  "keywords": [
    "duration",
    "react",
    "intl",
    "format"
  ],
  "homepage": "https://github.com/en-japan-air/react-intl-formatted-duration",
  "license": "MIT",
  "main": "dist/bundle.js",
  "module": "dist/module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/en-japan-air/react-intl-formatted-duration"
  },
  "files": [
    "dist",
    "index.js",
    "messages.js"
  ],
  "scripts": {
    "flow": "flow",
    "compile": "rollup -c rollup.config.js",
    "codecov": "codecov",
    "lint": "eslint index.js __tests__",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "prepare": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "codecov": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-react-intl": "^2.0.4",
    "eslint": "^5.15.3",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.95.1",
    "jest": "^24.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-intl": "^2.8.0",
    "react-test-renderer": "^16.8.5",
    "rollup": "1.7.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-filesize": "6.0.1",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-flow-entry": "0.3.1",
    "rollup-plugin-node-resolve": "4.0.1"
  },
  "peerDependencies": {
    "prop-types": ">= 15.0",
    "react": ">= 15.0",
    "react-intl": ">= 2.0",
    "intl-messageformat": ">= 2.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/internals/testing/test-setup.js"
    ],
    "testRegex": "__tests__/.*\\.test\\.js$",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "intl-unofficial-duration-unit-format": "1.1.0"
  }
}
