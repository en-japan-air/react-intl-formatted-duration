{
  "name": "react-intl-formatted-duration",
  "version": "1.1.0",
  "description": "React intl component to express time duration",
  "keywords": [
    "duration",
    "react",
    "intl",
    "format"
  ],
  "homepage": "https://github.com/en-japan-air/react-intl-formatted-duration",
  "license": "MIT",
  "main": "dist/bundle.js",
  "module": "dist/module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/en-japan-air/react-intl-formatted-duration"
  },
  "files": [
    "dist",
    "index.js",
    "messages.js"
  ],
  "scripts": {
    "compile": "rollup -c rollup.config.js",
    "codecov": "codecov",
    "lint": "eslint index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "prepare": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "babel-jest": "^22.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^1.4.8",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.2.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-test-renderer": "^16.2.0",
    "rollup": "0.55.4",
    "rollup-plugin-babel": "3.0.3"
  },
  "peerDependencies": {
    "prop-types": ">= 15.0",
    "react": ">= 15.0",
    "react-intl": ">= 2.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-setup.js",
    "testRegex": "__tests__/.*\\.test\\.js$",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  }
}
